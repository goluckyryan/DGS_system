field(INPA,"VME$(CRATE):$(BOARD):reg_live_timestamp_lsb_RBV.VAL PP")
field(INPB,"VME$(CRATE):$(BOARD):reg_live_timestamp_msb_RBV.VAL PP")
field(CALC,"(((A&2147483647)>>16) + ABS(A>>31)*32678 +(B<<16))/1525.8")
field(SCAN,"1 second")
field(DESC,"Timestamp Count in seconds")
field(OUT,"VME$(CRATE):$(BOARD):CV_LiveTS")
}
